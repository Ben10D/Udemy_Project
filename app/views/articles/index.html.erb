<!DOCTYPE html>
<html>
<head>
</head>
<body>
<div class="MainPage">
  <h1>Listing Aticles</h1>
 <%= link_to'New Article',new_article_path %>
 <%= link_to 'Download Article'%>
  <table>
    <tr>
      <th>Title</th>
      <th>Text</th>
      <th>Show</th>
      <th>Edit</th>
      <th>delete</th>
    </tr>
    <% @articles.each do|article|%><tr>
    <td><%= article.title%></td>
    <td><%= article.text%></td>
    <td><%= link_to 'Show',article_path(article) %></td>
    <td><%= link_to 'Edit', edit_article_path(article) %></td>
    <td><%= link_to 'Delete', article_path(article),
                      method: :delete,
                      data: {confirm:' Are u sure?'} %></td>
    <% end %></tr>

  </table>
</div>
</body>
</html>